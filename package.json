{
  "name": "blue-widgets",
  "version": "3.0.0-beta.4",
  "description": "A class-based widget library for adding complex javascript functionality to existing DOM elements",
  "files": [
    "es",
    "index.js",
    "registry.js",
    "parser.js",
    "Widget.js",
    "extend.js",
    "raf.js",
    "extendClass.js",
    "getData.js",
    "umd"
  ],
  "main": "index.js",
  "module": "es/index.js",
  "scripts": {
    "build": "node ./tools/build.js && npm run docs",
    "watch": "babel ./modules -d . --ignore __tests__ --watch",
    "docs": "jsdoc2md --template README.source.md ./modules/*.js > README.md",
    "prepublishOnly": "npm run build"
  },
  "author": {
    "name": "Simon Buerger",
    "url": "https://github.com/simonbuerger"
  },
  "license": "MIT",
  "issues": "http://github.com/bluegrassdigital/blue-widgets/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/bluegrassdigital/blue-widgets.git"
  },
  "keywords": [
    "dom",
    "widgets",
    "class",
    "library",
    "framework"
  ],
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/cli": "^7.0.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "gzip-size": "^5.0.0",
    "jsdoc-to-markdown": "^4.0.1",
    "pretty-bytes": "^5.1.0",
    "rollup": "^0.65.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^5.0.2"
  },
  "dependencies": {}
}
